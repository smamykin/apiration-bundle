{
  "name": "yapro/apiration",
  "type": "library",
  "description": "Api tool",
  "keywords": ["php", "api", "tool"],
  "license": "proprietary",
  "authors": [
    {
      "name": "Lebedenko Nikolay",
      "email": "totx@ya.ru",
      "role": "Developer",
      "homepage": "http://yapro.ru"
    }
  ],
  "require": {
    "php": "7.4.*",
    "yapro/marker-interfaces": "dev-main",
    "symfony/event-dispatcher": "*",
    "symfony/http-foundation": "*",
    "symfony/http-kernel": "*",
    "symfony/translation": "*",
    "doctrine/dbal": "*",
    "symfony/serializer": "*",
    "symfony/validator": "*",
    "laminas/laminas-code": "<4.0",
    "yapro/helpers": "*"
  },
  "autoload": {
    "psr-4": {
      "YaPro\\ApiRation\\": "src/"
    }
  },

  "require-dev": {
    "phpunit/phpunit": ">=6.0"
  },
  "autoload-dev": {
    "psr-4": {
      "YaPro\\ApiRation\\Tests\\": "tests/"
    }
  }
}
